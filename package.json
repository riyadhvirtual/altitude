{
  "name": "altitude",
  "version": "0.4.2",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "bunx --bun next build",
    "start": "bunx --bun next start",
    "cron": "bun run jobs/scheduler.ts",
    "bot": "bun run bot/index.ts",
    "bot:dev": "bun run --watch bot/index.ts",
    "bot:deploy": "bun run bot/deploy-commands.ts",
    "bot:start": "bun run bot/cli.ts start",
    "bot:stop": "bun run bot/cli.ts stop",
    "bot:restart": "bun run bot/cli.ts restart",
    "bot:status": "bun run bot/cli.ts status",
    "bot:test": "bun run bot/test-manager.ts",
    "lint": "bunx eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "bunx eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "bunx prettier --write .",
    "format:check": "bunx prettier --check .",
    "check:types": "bunx tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:setup": "bun run scripts/setup.ts",
    "db:seed": "bun run scripts/seed.ts",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "bun run lint:fix",
      "bun run format"
    ],
    "*.{json,md,css,scss}": [
      "bun run format"
    ]
  },
